script/source = "extends KinematicBody2D

func _process(delta: float) -> void:
	rotation = (get_global_mouse_position() - global_position).angle()
	
	var move: Vector2
	move.x = int(Input.is_action_pressed(\"right\")) - int(Input.is_action_pressed(\"left\"))
	move.y = int(Input.is_action_pressed(\"down\")) - int(Input.is_action_pressed(\"up\"))
	
	position += move.normalized() * 200 * delta

func _input(event: InputEvent) -> void:
	if Input.is_action_pressed(\"ui_accept\"):
		var ligh := preload(\"res://Darkning.tscn\").instance()
		ligh.from = position
		ligh.to = $\"../Lantern4\".position
		get_parent().add_child(ligh)
"

